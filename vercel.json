{
  "headers": [
    {
      "source": "/embed",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https: http:"
        }
      ]
    },
    {
      "source": "/embed/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https: http:"
        }
      ]
    }
  ]
}

